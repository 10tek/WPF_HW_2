    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ava1.jpg" />
    <None Remove="ava2.jpg" />
    <None Remove="ava3.jpg" />
